                p Usted esta sancionado en el tribunal de etica medica asi que no puede ejercer sus labores de medico
              p.estadoTitle(style="font-size: 20px; text-align: center; text-transform: uppercase; font-weight: bold;")  su estado es: 
                 span(style="color:red") #{datos.medico.estadoRegistro}
              #divpdf
                embed.evidencia(src="/pdfs/#{datos.medico.evidencias}" type="application/pdf")
              table.table
                form(id="info-estados" method='post' action="/medicos/estados")
                    input.form-control.input-formu(type ="hidden" name="ident" id="ident"  value="#{datos.medico.identificacion}")
                    .form-group
                      tr
                        td
                            label.control-label.col-sm-10(for="Estado" style="float: right;") Cambiar estado:
                        td
                          select.form-control.col-lg-12(id="estadoReg" name="estadoReg")
                            option(value="estudio") Estudio
                            option(value="aprobado" id="aprovar") Aprobado
                            option(value="desaprobado") Desaprobado
                        td
                          textarea.form-control.col-lg-12(name="observaciones" style="width100%; height:100px;") observaciones
                        td
                          input.btn-primary.btn(type="submit" value="Cambiar")

          #myModal.modal.fade(tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true")
            .modal-dialog.modal-lg( style="width: 60%;")
              .modal-content
                .modal-header
                  button(type="button" class="close" data-dismiss="modal" aria-hidden="true")&times;
                  h4(class="modal-title" id="myModalLabel") Editar Informacion personal personal del medico #{datos.medico.identificacion}
                .modal-body
