        $('#nit-edit').blur(function(){
          var nit =$(this).val();
            if(nit!=''){
              $.ajax({
                url: "/api/lugarTrabajo/nit?nit="+nit,
                type: 'get',
                success: function(data){
                  if(data.lugar){
                    $('#NombEmpres-edit').val(data.lugar.nombreClinica);
                    $("#municTrabajo-edit option").filter(function() {
                      return $(this).text() == data.lugar.ubicacion._municipio.nombre;
                    }).prop('selected', true);
                    $('#dirEmpresa-edit').val(data.lugar.ubicacion.direccion);
                    $('#celTrabajo-edit').val(data.lugar.telefono.celular);
                    $('#telTrabajo-edit').val(data.lugar.telefono.fijo);
                  }else if(data=="no OK"){
                    $('#NombEmpres-edit').val('');
                    $('#dirEmpresa-edit').val('');
                    $('#celTrabajo-edit').val('');
                    $('#telTrabajo-edit').val('');
                  }else {
                    alert('error');
                  }
                }
              });
            }
          });

